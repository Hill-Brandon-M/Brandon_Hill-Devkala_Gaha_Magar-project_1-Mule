        </types:variables>
      </types:input-event>
    </types:processor-declaration>
    <types:operation-declaration>
      <types:inputs>
        <types:parameter name="value" type="invoice[]"/>
      </types:inputs>
    </types:operation-declaration>
  </types:enrichment>
</types:mule>