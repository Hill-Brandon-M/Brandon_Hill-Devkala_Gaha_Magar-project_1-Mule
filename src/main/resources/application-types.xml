        </types:message>
      </types:input-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#a669ac0f-a9e3-4107-b271-64df7f785182">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="invoice"/>
